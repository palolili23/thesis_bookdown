  author = {Johnson, Eric S. and Bartman, Barbara A. and Briesacher, Becky A. and Fleming, Neil S. and Gerhard, Tobias and Kornegay, Cynthia J. and Nourjah, Parivash and Sauer, Brian and Schumock, Glen T. and Sedrakyan, Art and St{\"u}rmer, Til and West, Suzanne L. and Schneeweiss, Sebastian},
  year = {2013},
