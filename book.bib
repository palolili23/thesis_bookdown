  author = {Kaufman, Jay S. and Kaufman, Sol and Poole, Charles},
  year = {2003},
  month = dec,
  journal = {Social Science \& Medicine},
  volume = {57},
  number = {12},
  pages = {2397--2409},
  issn = {02779536},
  doi = {10.1016/S0277-9536(03)00135-7},
  abstract = {Social epidemiology is the study of relations between social factors and health status in populations. Although recent decades have witnessed a rapid development of this research program in scope and sophistication, causal inference has proven to be a persistent dilemma due to the natural assignment of exposure level based on unmeasured attributes of individuals, which may lead to substantial confounding. Some optimism has been expressed about randomized social interventions as a solution to this long-standing inferential problem. We review the causal inference problem in social epidemiology, and the potential for causal inference in randomized social interventions. Using the example of a currently on-going intervention that randomly assigns families to non-poverty housing, we review the limitations to causal inference even under experimental conditions and explain which causal effects become identifiable. We note the benefit of using the randomized trial as a conceptual model, even for design and interpretation of observational studies in social epidemiology.},
  language = {en},
  file = {C\:\\Users\\040609\\Zotero\\storage\\DUIXGTTX\\Kaufman et al. - 2003 - Causal inference from randomized trials in social .pdf}
}

