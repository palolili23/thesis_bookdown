@article{musicco2013,
  title = {Inverse Occurrence of Cancer and {{Alzheimer}} Disease: {{A}} Population-Based Incidence Study},
  shorttitle = {Inverse Occurrence of Cancer and {{Alzheimer}} Disease},
  author = {Musicco, M. and Adorni, F. and Di Santo, S. and Prinelli, F. and Pettenati, C. and Caltagirone, C. and Palmer, K. and Russo, A.},
