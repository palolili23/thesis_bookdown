  author = {Ballard, Clive and Aarsland, Dag and Cummings, Jeffrey and O'Brien, John and Mills, Roger and Molinuevo, Jose Luis and Fladby, Tormod and Williams, Gareth and Doherty, Pat and Corbett, Anne and Sultana, Janet},
